{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
  	"node": true,
  	"es6": true
  },
  "rules": {
    "camelcase": 0,
    "semi": [2, "always"],
    "eqeqeq": 2,
    "no-trailing-spaces": [1, {"skipBlankLines": true}],
    "no-empty": [2, {"allowEmptyCatch": true}]
  }
}